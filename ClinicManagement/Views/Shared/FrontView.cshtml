
<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" href="~/Client/img/download.png" type="image/png">
    <title>Centric Medical</title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/Client/css/bootstrap.css">
    <link rel="stylesheet" href="/Client/vendors/linericon/style.css">
    <link rel="stylesheet" href="/Client/css/font-awesome.min.css">
    <link rel="stylesheet" href="/Client/vendors/owl-carousel/owl.carousel.min.css">
    <link rel="stylesheet" href="/Client/vendors/lightbox/simpleLightbox.css">
    <link rel="stylesheet" href="/Client/vendors/nice-select/css/nice-select.css">
    <link rel="stylesheet" href="/Client/vendors/animate-css/animate.css">
    <link rel="stylesheet" href="/Client/vendors/jquery-ui/jquery-ui.css">
    <!-- main css -->
    <link rel="stylesheet" href="/Client/css/style.css">
    <link rel="stylesheet" href="/Client/css/responsive.css">
   
</head>

<body>

    <!--================Header Menu Area =================-->
    <header class="header_area">
        <div class="top_menu row m0">
            <div class="container-fluid">
                <div class="float-left">
                    <p>Call Us: 012 44 5698 7456 896</p>
                </div>
                <div class="float-right">
                    <ul class="right_side">
                        <li>
                            <a href="@Url.Action("ClientLogin", "Front")" title="Login">
                                Login/Register
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("About","Front")">
                                About Us
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="main_menu">
            <nav class="navbar navbar-expand-lg navbar-light">
                <div class="container-fluid">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <a class="navbar-brand logo_h" href="@Url.Action("Index", "Front")">
                        <img src="~/Client/img/download.png" height="70" width="150" alt="">
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse offset" id="navbarSupportedContent"> 
                        <div class="row w-100">
                            <div class="col-lg-7 pr-0">
                                <ul class="nav navbar-nav center_nav pull-right">
                                    <li class="nav-item">
                                        <a class="nav-link" href="@Url.Action("Index", "Front")">Home</a>
                                    </li>
                                    <li class="nav-item submenu dropdown">
                                       <a href="@Url.Action("AllProducts", "Front")" class="nav-link dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Products</a>
                                        <ul class="dropdown-menu">
                                            @{
                                                var cat = new ClinicManagement.Models.ClinicManagmentSystemEntities().Categories.ToList();

                                                foreach (var item in cat)
                                                {


                                                    <li class="nav-item">
                                                        <a class="nav-link" href="@Url.Action("Product", "Front", new { id = item.Id })">@item.Name</a>
                                                    </li>
                                                }

                                            }
                                        </ul>
         
                                               <li class="nav-item submenu dropdown">
                                        <a href="/Client/#" class="nav-link dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Education</a>
                                        <ul class="dropdown-menu">
                                            @{
                                                var edu = new ClinicManagement.Models.ClinicManagmentSystemEntities().EducationTypes.ToList();
                                                foreach (var item in edu)
                                                {
                                                    <li class="nav-item">
                                                        <a class="nav-link" href="@Url.Action("Educations", "FrontEducations", new { id = item.Id })">@item.Name</a>
                                                    </li>
                                                }
                                            }
                                            
                                            
                                        </ul>
                                    </li>
                                    
                                    <li class="nav-item">
                                        <a class="nav-link" href="@Url.Action("Index","Feedbacks")">Feedback</a>
                                    </li>
                                       
                                    <li class="nav-item">
                                        <a class="nav-link" href="@Url.Action("Contact","Front")">Contact</a>
                                    </li>

                                  
                                        
                                </ul>
                            </div>

                            <div class="col-lg-5">
                                <ul class="nav navbar-nav navbar-right right_nav pull-right">
                                    <hr>
                                  

                                    <hr>

                                    <li class="nav-item">
                                        <a href="@Url.Action("ClientSignup","Front")" title="Create account" class="icons">
                                            <i class="fa fa-user-plus" aria-hidden="true"></i>
                                        </a>
                                    </li>

                                    <hr>


                                    <hr>

                                    <li class="nav-item">
                                        <a href="@Url.Action("Cart","Front")" title="View cart" class="icons">
                                            <i class="lnr lnr lnr-cart"></i>
                                        </a>
                                    </li>

                                    <hr>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </nav>
        </div>
    </header>
    <!--================Header Menu Area =================-->
    @RenderBody()
    <!--================ start footer Area  =================-->
    <footer class="footer-area section_gap">
        <div class="container">
            <div class="row">
                <div class="col-lg-3  col-md-6 col-sm-6">
                    <div class="single-footer-widget">
                        <h6 class="footer_title">About Us</h6>
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore dolore magna aliqua.</p>
                    </div>
                </div>
              
                <div class="col-lg-4 col-md-6 col-sm-6">
                    <h6 class="footer_title">Pages</h6>
                    <ul>
                        <li style="list-style:none;">
                            <a style="color:grey;" href="@Url.Action("Index","Front")">Home</a>
                        </li>
                        <li style="list-style:none;">
                            <a style="color:grey;" href="@Url.Action("AllProducts","Front")">Products</a>
                        </li>
                        <li  style="list-style:none;">
                            <a style="color:grey;" href="@Url.Action("Educations","FrontEducations")">Education</a>
                        </li>
                        <li  style="list-style:none;">
                            <a style="color:grey;" href="@Url.Action("Index","Feedbacks")">Feedback</a>
                        </li>
                        <li  style="list-style:none;">
                            <a style="color:grey;" href="@Url.Action("Contact","Front")">Contact</a>
                        </li>
                    </ul>
                </div>
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="single-footer-widget instafeed">
                        <h6 class="footer_title">Gallery</h6>
                        <ul class="list instafeed d-flex flex-wrap">
                            @{ 
                                var db = new ClinicManagement.Models.ClinicManagmentSystemEntities().Images.Take(8).ToList();
                            }
                            @foreach (var item in db)
                            {
                            <li>
                                <img src="~/Content/Images/@item.Imagename" alt="" height="50" width="50">
                            </li>
                            }
                        </ul>
                    </div>
                </div>
                <div class="col-lg-2 col-md-6 col-sm-6">
                    <div class="single-footer-widget f_social_wd">
                        <h6 class="footer_title">Follow Us</h6>
                        <p>Let us be social</p>
                        <div class="f_social">
                            <a href="/Client/#">
                                <i class="fa fa-facebook"></i>
                            </a>
                            <a href="/Client/#">
                                <i class="fa fa-twitter"></i>
                            </a>
                            <a href="/Client/#">
                                <i class="fa fa-dribbble"></i>
                            </a>
                            <a href="/Client/#">
                                <i class="fa fa-behance"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row footer-bottom d-flex justify-content-between align-items-center">
                <p class="col-lg-12 footer-text text-center">
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    Copyright &copy;
                    <script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="/Client/https://colorlib.com" target="_blank">Colorlib</a>
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                </p>
            </div>
        </div>
    </footer>
    <!--================ End footer Area  =================-->
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="/Client/js/jquery-3.2.1.min.js"></script>
    <script src="/Client/js/popper.js"></script>
    <script src="/Client/js/bootstrap.min.js"></script>
    <script src="/Client/js/stellar.js"></script>
    <script src="/Client/vendors/lightbox/simpleLightbox.min.js"></script>
    <script src="/Client/vendors/nice-select/js/jquery.nice-select.min.js"></script>
    <script src="/Client/vendors/isotope/imagesloaded.pkgd.min.js"></script>
    <script src="/Client/vendors/isotope/isotope-min.js"></script>
    <script src="/Client/vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="/Client/js/jquery.ajaxchimp.min.js"></script>
    <script src="/Client/vendors/counter-up/jquery.waypoints.min.js"></script>
    <script src="/Client/vendors/flipclock/timer.js"></script>
    <script src="/Client/vendors/counter-up/jquery.counterup.js"></script>
    <script src="/Client/js/mail-script.js"></script>
    <script src="/Client/js/theme.js"></script>
</body>

</html>