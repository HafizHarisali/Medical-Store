<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                ORDER DETAILS
            </div>
            <br />
            <div class="row">
                <div class="col-lg-8"></div>
                <div class="col-lg-3" style="margin-left:6%;">
                    <div class="input-group custom-search-form">
                        <input type="text" class="form-control" id="Search" placeholder="Search..." name="search">

                    </div>
                </div>
                <div class="col-lg-1"></div>
            </div>

            <!-- /.panel-heading -->
            <div class="panel-body">
                <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr>
                            <th>Order Number</th>
                            <th>Product Id</th>
                            <th>Product Name</th>
                            <th>Quantity</th>
                            <th>Price</th>
                            <th>Amount</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in ViewBag.orderdetail)
                        {
                            <tr class="odd gradeX Search">
                                <td>@item.Orderid</td>
                                <td>@item.Productid</td>
                                <td>@item.ProductName</td>
                                <td>@item.Productqty</td>
                                <td class="center">@item.Productprice</td>
                                <td class="center">@item.Productamount</td>
                            </tr>
                        }


                    </tbody>
                </table>
                <!-- /.table-responsive -->

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                ORDERS
            </div>
            <br />
            <div class="row">
                <div class="col-lg-8"></div>
                <div class="col-lg-3" style="margin-left:6%;">
                    <div class="input-group custom-search-form">
                        <input type="text" class="form-control" id="Searching" placeholder="Search..." name="search">




                    </div>
                </div>
                <div class="col-lg-1"></div>
            </div>

            <!-- /.panel-heading -->
            <div class="panel-body">
                <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr>
                            <th>Order</th>
                            <th>First Name</th>
                            <th>Last name</th>
                            <th>Clinic</th>
                            <th>Contact</th>
                            <th>Address</th>
                            <th>City</th>
                            <th>Date</th>
                            <th>Amount</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in ViewBag.order)
                        {
                            <tr class="odd gradeX Searching">
                                <td>@item.Id</td>
                                <td>@item.Firstname</td>
                                <td>@item.Lastname</td>
                                <td>@item.Clinicname</td>
                                <td>@item.Phone</td>
                                <td>@item.Address</td>
                                <td>@item.City</td>
                                <td class="center">@item.Date</td>
                                <td class="center">@item.Amount</td>
                            </tr>
                        }


                    </tbody>
                </table>
                <!-- /.table-responsive -->

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
